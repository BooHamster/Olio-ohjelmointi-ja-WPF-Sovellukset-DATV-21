                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FFFF00F2" Offset="1"/>
                </LinearGradientBrush>
            </Button.Foreground>
        </Button>
        <Label Content="mm" HorizontalAlignment="Left" Margin="163.667,24,0,0" VerticalAlignment="Top" Width="38" Height="23" Grid.Column="1" FontFamily="Rockwell Extra Bold" Grid.ColumnSpan="3">

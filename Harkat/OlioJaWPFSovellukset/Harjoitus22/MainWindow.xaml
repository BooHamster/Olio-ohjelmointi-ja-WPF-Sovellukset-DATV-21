        <Button x:Name="AsetaArvot" Content="AsetaArvot" HorizontalAlignment="Left" Margin="35,265,0,0" VerticalAlignment="Top" Width="251" Click="AsetaArvot_Click" FontFamily="Rockwell Extra Bold" Height="32" FontSize="24" Grid.ColumnSpan="2" Background="#FFDD851A">

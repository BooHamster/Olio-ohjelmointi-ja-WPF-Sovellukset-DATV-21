                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF010101" Offset="1"/>
                </LinearGradientBrush>
            </TextBlock.Foreground>
        </TextBlock>
        <TextBlock x:Name="KarminPiiriTeksti" HorizontalAlignment="Left" Margin="76.667,218,0,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="-0.367,0.593" Width="105" Height="23" Grid.Column="1" FontFamily="Rockwell Extra Bold" Grid.ColumnSpan="2">
            <TextBlock.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="Black" Offset="1"/>
                </LinearGradientBrush>

        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Harjoitus22"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="801.9" WindowStartupLocation="CenterOwner">
    <Grid x:Name="Asetukset" Margin="50,35,221.333,24.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="173*"/>
            <ColumnDefinition Width="176*"/>
            <ColumnDefinition Width="18*"/>
            <ColumnDefinition Width="50*"/>
        </Grid.ColumnDefinitions>
        <Grid.Background>
            <ImageBrush ImageSource="https://4.imimg.com/data4/NF/SF/MY-16570734/wooden-window-500x500.jpg"/>
        </Grid.Background>
        <TextBox x:Name="IkkunanLeveysBox" HorizontalAlignment="Left" Height="23" Margin="76.667,24,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82" PreviewTextInput="IkkunanLeveysBox_PreviewTextInput" Grid.Column="1"/>
        <TextBox x:Name="IkkunanKorkeusBox" HorizontalAlignment="Left" Height="23" Margin="76.667,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="82" PreviewTextInput="IkkunanKorkeusBox_PreviewTextInput" Grid.Column="1"/>
